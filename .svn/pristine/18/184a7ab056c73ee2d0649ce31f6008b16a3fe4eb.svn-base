						  from RNO_GSM_MRR_QUALITY qua, RNO_GSM_MRR_DESC des
						 where qua.eri_mrr_desc_id = des.id
						   and qua.eri_mrr_desc_id = #{mrrDescId}
						   and qua.mea_date = #{meaTime}
						   and qua.city_id = #{cityId}
